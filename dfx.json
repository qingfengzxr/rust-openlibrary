{
  "canisters": {
    "openlibrary_backend": {
      "candid": "src/openlibrary_backend/openlibrary_backend.did",
      "package": "openlibrary_backend",
      "type": "rust"
    },
    "openlibrary_frontend": {
      "dependencies": [
        "openlibrary_backend"
      ],
      "frontend": {
        "entrypoint": "src/openlibrary_frontend/src/index.html"
      },
      "source": [
        "src/openlibrary_frontend/assets",
        "dist/openlibrary_frontend/"
      ],
      "type": "assets"
    },
    "openlibrary_book": {
      "candid": "src/openlibrary_book/openlibrary_book.did",
      "package": "openlibrary_book",
      "type": "rust"
    },
    "store_backend": {
      "candid": "src/store_backend/store_backend.did",
      "package": "store_backend",
      "type": "rust"
    },
    "hello_backend": {
      "candid": "src/hello_backend/hello_backend.did",
      "package": "hello_backend",
      "type": "rust"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}